{
  "batch_size": 8,
  "learning_rate": 0.001,
  "num_epochs": 50,
  "warmup_epochs": 5,
  "use_mixed_precision": false,
  "grad_scaler_init_scale": 65536,
  "optimizer_type": "adamw",
  "weight_decay": 1e-05,
  "gradient_clip_value": 1.0,
  "scheduler_type": "onecycle",
  "use_channels_last": true,
  "compile_model": true,
  "benchmark_cudnn": true,
  "num_workers": 16,
  "pin_memory": true,
  "persistent_workers": true,
  "prefetch_factor": 4,
  "save_every_n_epochs": 10,
  "log_every_n_steps": 5,
  "validate_every_n_epochs": 5,
  "early_stopping_patience": 15,
  "early_stopping_delta": 0.0001,
  "model_a_loss_weights": {
    "coord_weight": 1.0,
    "edge_weight": 1.0,
    "count_weight": 0.1,
    "regularization_weight": 0.001
  },
  "model_b_loss_weights": {
    "coord_weight": 1.0,
    "edge_weight": 2.0,
    "count_weight": 0.1,
    "similarity_weight": 0.3,
    "temperature_reg": 0.01
  },
  "device": "cpu",
  "multi_gpu": false
}